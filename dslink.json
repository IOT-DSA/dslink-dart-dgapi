{
  "name": "dslink-dart-dgapi",
  "version": "1.0.6",
  "description": "DGAPI DSLink",
  "main": "bin/run.dart",
  "engines": {
    "dart": ">=1.13.0"
  },
  "getDependencies": [],
  "configs": {
    "broker": {
      "type": "url"
    },
    "name": {
      "type": "string",
      "default": "dgapi"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "token": {
      "type": "string"
    },
    "poll-interval": {
      "type": "number",
      "default": 1000
    }
  }
}
